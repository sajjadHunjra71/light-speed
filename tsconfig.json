{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "src/*": [
        "./src/*"
      ],
      "tailwind.config": [
        "./tailwind.config.js"
      ],
      "@components": [
        "./src/components"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@hooks": [
        "./src/hooks"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@services": [
        "./src/services"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@types": [
        "./src/types"
      ],
      "@types/*": [
        "./src/types/*"
      ],
      "@utils": [
        "./src/utils"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@theme": [
        "./src/constants/theme"
      ],
      "@theme/*": [
        "./src/constants/theme/*"
      ],
      "@ui-kit": [
        "./src/components/ui-kit.ts"
      ],
      "@store": [
        "./src/store"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@constants": [
        "./src/constants"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@screens": [
        "./src/screens"
      ],
      "@screens/*": [
        "./src/screens/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}

